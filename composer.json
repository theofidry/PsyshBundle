{
    "name": "theofidry/psysh-bundle",
    "type": "symfony-bundle",
    "description": "A port of the PHP REPL PsySH for Symfony.",
    "keywords": ["console", "psysh", "shell", "REPL", "symfony", "interactive"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Palmer",
            "email": "navitronic@gmail.com"
        },
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],

    "require": {
        "php": "^7.1",
        "psy/psysh": "^0.8 || ^0.9",
        "symfony/error-handler": "^4.3|^4.4|^5.0",
        "symfony/expression-language": "^4.3|^5.0",
        "symfony/framework-bundle": "^4.3|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/symfony": "^4.3|^5.0"
    },

    "autoload": {
        "psr-4": {
            "Fidry\\PsyshBundle\\": "src"
        },
        "files": [
            "src/psysh.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fidry\\PsyshBundle\\": "tests"
        }
    },

    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
